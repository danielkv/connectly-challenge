{
	"trailingComma": "es5",
	"tabWidth": 4,
	"semi": false,
	"printWidth": 120,
	"singleQuote": true,
	"plugins": [
		"@trivago/prettier-plugin-sort-imports"
	],
	"importOrder": [
		"^react",
		"^[a-z]",
		"^@.*",
		"^.*$",
		"^@(.*)$",
		"^../",
		"^./"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}